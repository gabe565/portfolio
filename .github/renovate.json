{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "local>gabe565/renovate-config"
  ],
  "packageRules": [
    {
      "matchPackageNames": ["vue"],
      "allowedVersions": "< 3"
    },
    {
      "matchPackageNames": ["vue-router"],
      "allowedVersions": "< 4"
    },
    {
      "matchPackageNames": ["vue-loader"],
      "allowedVersions": "< 16"
    },
    {
      "matchPackageNames": ["@fortawesome/vue-fontawesome"],
      "allowedVersions": "< 3"
    }
  ],
  "hostRules": [
    {
      "matchHost": "https://npm.fontawesome.com/",
      "hostType": "npm",
      "encrypted": {
        "token": ""
      }
    }
  ]
}
